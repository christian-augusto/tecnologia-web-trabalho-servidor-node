{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "16.17.0"
        }
      }
    ],
    "@babel/preset-typescript",
    "minify"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@tests": "./src/tests",
          "@config": "./src/config",
          "@entities": "./src/infra/entities"
        }
      }
    ]
  ],
  "ignore": ["./src/tests"]
}
